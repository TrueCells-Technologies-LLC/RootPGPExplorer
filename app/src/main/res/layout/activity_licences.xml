<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_licences"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.osama.cryptofm.about.LicencesActivity">
<TextView
    android:layout_margin="5dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Spongy castle"
    android:textAppearance="@android:style/TextAppearance.Material.Medium"
    android:id="@+id/spongy_castle_license"
    android:onClick="licenseClick"
    />
    <TextView
        android:layout_margin="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Numix Icons"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:id="@+id/numix_icon_license"
        android:onClick="licenseClick"
        />
        <TextView
            android:layout_margin="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Easy permission"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:id="@+id/easy_permission_license"
            android:onClick="licenseClick"
        />
        <TextView
            android:layout_margin="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sqlcipher"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        android:id="@+id/sqlcipher_license"
            android:onClick="licenseClick"
        />
</LinearLayout>
